{
	"name": "moibot",
	"version": "0.0.1",
	"main": "src/index.js",
	"description": "A Community Discord Bot",
	"scripts": {
		"dev": "nodemon -L --exec babel-node src/index.js",
		"build": "babel src -d dist",
		"start": "yarn build && node dist/index.js",
		"build:docker": "docker-compose build",
		"start:docker": "docker-compose up -d",
		"end:docker": "docker-compose down",
		"clean:docker": "docker system prune -f",
		"test": "jest"
	},
	"license": "MIT",
	"dependencies": {
		"@babel/polyfill": "^7.4.4",
		"axios": "^0.19.0",
		"bufferutil": "^4.0.1",
		"cheerio": "^1.0.0-rc.3",
		"cors": "^2.8.5",
		"dotenv": "^8.0.0",
		"eris": "^0.11.0",
		"eventemitter3": "^4.0.0",
		"express": "^4.17.0",
		"lodash": "^4.17.11",
		"pako": "^1.0.10",
		"sodium-native": "^2.4.6",
		"woke-dyno": ">=0.0.4"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.0.0",
		"@babel/node": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"babel-jest": "^24.9.0",
		"jest": "^24.9.0",
		"nodemon": "^1.19.0",
		"yarn": "^1.22.0"
	},
	"engines": {
		"node": "10.x"
	}
}
