{
  "name": "moibot",
  "version": "0.0.2",
  "main": "src/index.js",
  "description": "A Community Discord Bot",
  "scripts": {
    "dev": "nodemon -L --exec babel-node src/index.js",
    "build": "babel src -d dist",
    "start": "yarn build && node dist/index.js",
    "dev:docker": "docker-compose up -d && yarn test",
    "build:docker": "docker-compose build",
    "start:docker": "docker-compose up -d",
    "end:docker": "docker-compose down",
    "clean:docker": "docker system prune -f",
    "test": "jest --watch"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "bufferutil": "^4.0.1",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eris": "^0.11.2",
    "eventemitter3": "^4.0.0",
    "express": "^4.17.1",
    "jsdom": "^16.2.0",
    "lodash": "^4.17.19",
    "ml5": "^0.4.3",
    "pako": "^1.0.11",
    "showdown": "^1.9.1",
    "sodium-native": "^3.0.0",
    "woke-dyno": ">=0.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "jest": "^25.1.0",
    "nodemon": "latest",
    "superagent": "^5.2.2",
    "supertest": "^4.0.2",
    "yarn": "latest"
  }
}
